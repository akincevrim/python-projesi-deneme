menu = """
1-toplama 
2-çıkarma
3-çarpma
4-bölme
5-faktöriyel
6-kuvvet
7-kök alma
8-çıkış
"""
anahtar = 1
while anahtar == 1:
    işlem = int(input(menu))
    if işlem !=8:
        sayı1 = int(input("1.sayıyı giriniz"))
        sayı2 = int(input("2.sayıyı giriniz"))
        if işlem == 1:
            print(f"{sayı1} + {sayı2} = {sayı1+sayı2}")
        elif işlem == 2:
            print(f"{sayı1} - {sayı2} = {sayı1-sayı2}")
        elif işlem == 3:
            print(f"{sayı1} * {sayı2} = {sayı1*sayı2}")
        elif işlem == 4:
            print(f"{sayı1} / {sayı2} = {sayı1/sayı2}")
        elif işlem == 5:
            for i in range(1,sayı1+1):
                    sonuc *= i
            print(f"{sayı1}'in faktöriyeli {sonuc}")
        elif işlem == 6:
            print(f"{sayı1}'in {sayı2}.kuvveti:{sayı1**sayı2}")
        elif işlem == 7:
            print(f"{sayı1}'in karekökü {sayı1**(1/2)}")
        elif işlem == 8:
            break
        else: 
            anahtar = 0
    else:
        print("program bitti")

