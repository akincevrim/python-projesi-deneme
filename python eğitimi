sayı = 25
hak = 0
while hak < 5 :
    tahmin = int(input("tahmininizi giriniz:"))
    hak += 1
    if tahmin == sayı:
        print("tebrikler")
        break
    elif tahmin < sayı:
        print("daha yüksek tahmin yapınız kalan hak {}".format(hak))
    elif tahmin > sayı:
        print("daha küçük bir tahmin yapınız kalan hak {}".format(hak))
else:
    print("oyun bitti")
