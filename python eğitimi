def fonk (sayılar):
    sonuc = 0
    for item in liste:
        sonuc += item
    return sonuc
print(fonk([1,2,3,4,5,6]))
21
def fonk(*args):
    sonuc = 0
    for item in args:
        sonuc += item
    return sonuc
print(fonk(1,2,3,4,5,6))
21
def fonk(*args):
    liste = []
    for item in args:
        if item %2 == 0:
            liste.append(item)
    return liste
sonuc = fonk(1,3,4,5,69,9,22)
print(*sonuc)
4 22
def fonk(*args):
    print(type(args))
sonuc = fonk(1,3,4,5,69,9,22)
<class 'tuple'>
liste = {"isim","soyisim","il"}
sözlük = dict.fromkeys(liste,"")
def fonk(*args):
    for i in range(0,len(args)):
        if i == 0:
            sözlük["isim"] = args[i]
        elif i == 1:
            sözlük["soyisim "]= args[i]
        elif i == 2:
            sözlük["il"]=args[i]
fonk("ali","veli","izmir")
print(sözlük)
{'il': 'izmir', 'isim': 'ali', 'soyisim': '', 'soyisim ': 'veli'}
