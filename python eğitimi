def faktöriyel(sayı):
    sonuc = 1
    for i in range(1,sayı+1):
        sonuc *= i
    return sonuc
faktöriyel(3)
def faktöriyelözyineleme(sayı):
    if sayı == 1:
        return sayı
    else:
        return sayı * faktöriyelözyineleme (sayı-1)
faktöriyelözyineleme(66)
def toplama(sayı,mevcut):
    print(f"sayı:{sayı},mevcut:{mevcut},sayı+mevcut:{sayı+mevcut}")
    if sayı == 9:
        return mevcut
    else:
        print("fonksiyon tekrarı")
        return toplama (sayı+1,mevcut+sayı)
toplama(1,0)
def toplama(sayı,mevcut,limit):
    print(f"sayı:{sayı},mevcut:{mevcut},sayı+mevcut:{sayı+mevcut}")
    if sayı == limit:
        return mevcut
    else:
        print("fonksiyon tekrarı")
        return toplama (sayı+1,mevcut+sayı,limit)
toplama(1,0,11)
